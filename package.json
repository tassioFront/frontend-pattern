{
  "name": "frontend-patterns",
  "repository": "https://github.com/tassioFront/frontend-pattern.git",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test:ci": "vitest --watch=false",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "dev": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "visualizer": "VITE_VISUALIZER=true yarn build",
    "e2e": "cypress run --browser chrome",
    "pre-commit": "lint-staged",
    "pre-push": "yarn test:ci && yarn build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@open-ish/utility-storagefy": "1.1.1",
    "@open-ish/utility-trycatchfy": "1.3.2",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.1.3",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lazy-load-image-component": "^1.5.6",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "uniqid": "^5.4.0",
    "vite": "5.2.11",
    "web-vitals": "^2.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "14.2.2",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@types/uniqid": "^5.3.2",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "cypress": "^12.9.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.35.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "styled-components": "^5.3.6",
    "styled-media-query": "^2.1.2",
    "typescript": "4.8.4",
    "vitest": "1.6.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": "prettier --write --ignore-unknown"
  }
}
